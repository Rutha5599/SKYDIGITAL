<%-include('manager-header')%>
<div class="clearfix"></div>
	
  <div class="content-wrapper">
    <div class="container-fluid">

    <div class="row mt-3">
      <div class="col-lg-12">
         <div class="card">
           <div class="card-body">
           <div class="card-title">Vertical Form</div>
           <hr>
            <form action="/manager/studentdata" method="post" enctype="multipart/form-data">
           <div class="form-group">
            <label for="input-1">Student Name</label>
            <input type="text" name="studentname" class="form-control" id="input-1" placeholder="Enter Your Student Name">
           </div>
           <div class="form-group">
            <label for="input-2">FATHER NAME</label>
            <input type="text" name="fathername" class="form-control" id="input-2" placeholder="Enter Your Father Name">
           </div>
           <div class="form-group">
            <label for="input-2">Email ID</label>
            <input type="email" name="studentemailid" class="form-control" id="input-3" placeholder="Enter Your Email ID">
           </div>
           <div class="form-group">
            <label for="input-3">STUDENT MOBILE Number</label>
            <input type="number" name="studentmobile" class="form-control" id="input-4" placeholder="Enter Your Student Mobile Number">
           </div>
           <div class="form-group">
            <label for="input-4">FATHER MOBILE Number</label>
            <input type="number" name="fathermobile" class="form-control" id="input-5" placeholder="Enter Your Father Mobile Number">
           </div>
           <div class="form-group">
            <label for="input-4">BANCH TIME</label>
            <select name="banchtime" class="form-control" style="color:#a1bdd2 !important;">
                <option value="">--- Select Banch Time ---</option>
                <option value="08:00 AM">08:00 AM</option>
                <option value="09:00 AM">09:00 AM</option>
                <option value="10:00 AM">10:00 AM</option>
                <option value="11:00 AM">11:00 AM</option>
                <option value="12:00 PM">12:00 PM</option>
                <option value="01:00 PM">01:00 PM</option>
                <option value="02:00 PM">02:00 PM</option>
                <option value="03:00 PM">03:00 PM</option>
                <option value="04:00 PM">04:00 PM</option>
                <option value="05:00 PM">05:00 PM</option>
                <option value="06:00 PM">06:00 PM</option>
                <option value="07:00 PM">07:00 PM</option>
            </select>
           </div>
           <div class="form-group">
            <label for="input-4">COURSE</label>
            <input type="text" name="course" class="form-control" id="input-4" placeholder="Enter Your Student Course">
           </div>
           <div class="form-group">
            <label for="input-4">FEES</label>
            <input type="number" name="fees" class="form-control" id="input-4" placeholder="Enter Your Student Fees">
           </div>
           <div class="form-group">
            <label for="input-4">Student Image</label>
            <input type="file" name="img" class="form-control" id="input-4" style="color:#a1bdd2 !important;">
           </div>
           <div class="form-group">
            <label for="input-5">ADDMISION DATE</label>
            <input type="date" name="addmisiondate" class="form-control" id="input-5" style="color:#a1bdd2 !important;">
           </div>
           <div class="form-group py-2">
             <div class="icheck-material-white">
            <input type="checkbox" id="user-checkbox1" checked=""/>
            <label for="user-checkbox1">I Agree Terms & Conditions</label>
            </div>
           </div>
           <div class="form-group">
            <input type="submit" value="Submit" class="btn btn-light px-5" >
          </div>
          </form>
         </div>
         </div>
      </div>
    </div><!--End Row-->

	<%-include('footer')%>